<h1>All Interview Requests </h1>
<% for  all_applicants in @all_applicants %>
    <% if current_user.id == all_applicants.id  %>
        <% next %>
    <% elsif all_applicants.applicant_type=="Candidate" %>
        <%= link_to all_applicants.name, user_profile_path(all_applicants.id) %>
    <% else %>
        <% next %>
    <% end %>
<% end %>